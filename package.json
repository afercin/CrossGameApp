{
  "name": "crossgame",
  "version": "0.0.0",
  "main": "main.js",
  "bin": "main.js",
  "email": "dvm@seaberyat.com",
  "author": "Seabery <dvm@seaberyat.com>",
  "homepage": ".",
  "description": "App to incurr in Kenjo",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "ng build && electron .",
    "package-mac": "electron-packager . AutoKenjo --overwrite --asar=true --platform=darwin --arch=x64 --icon=release/assets/icons/darwin/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . AutoKenjo --overwrite --asar=true --platform=win32 --arch=x64 --icon=./release/assets/icons/win32/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"AutoKenjo\"",
    "package-lin": "electron-packager . AutoKenjo --overwrite --asar=true --platform=linux --arch=x64 --icon=release/assets/icons/linux/icon.png --prune=true --out=release-builds",
    "dist": "ng build && electron-builder"
  },
  "build": {
    "appId": "Kenjo",
    "extraFiles": [
      "kenjo.properties"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "deb"
      ],
      "icon": "./release/assets/icons/linux/icon.png",
      "category": "Utility"
    },
    "win": {
      "target": "NSIS",
      "icon": "./release/assets/icons/win32/icon.ico"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "electron-root-path": "^1.0.16",
    "properties-reader": "^2.2.0",
    "puppeteer": "^13.3.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.4",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "electron": "^16.0.7",
    "electron-builder": "^22.8.0",
    "electron-packager": "^15.4.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2",
    "zone.js": "^0.11.4"
  }
}
